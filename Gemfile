source "https://rubygems.org"

gem "jekyll", "3.9.3"
gem "github-pages", group: :jekyll_plugins

# Usando SSH (recomendado para repositórios privados)
gem 'jekyll-theme-conexoes', '~> 1.1', git: 'git@github.com:jekyll/jekyll-theme-conexoes.git'

# Usando HTTPS com token (recomendado para repositórios privados)
gem 'jekyll-theme-conexoes', '~> 1.1', git: 'https://github.com/jekyll/jekyll-theme-conexoes.git', token: ENV['SEU_TOKEN']
